                                 	; --- Memory layout in SRAM
                                 	.dseg
                                 	.org	SRAM_START
000060                           POSX:	.byte	1	; Own position
000061                           POSY:	.byte 	1
000062                           TPOSX:	.byte	1	; Target position
000063                           TPOSY:	.byte	1
000064                           LINE:	.byte	1	; Current line	
000065                           VMEM:	.byte	VMEM_SZ ; Video MEMory
00006a                           SEED:	.byte	1	; Seed for Random
                                 
                                 	; ---------------------------------------
                                 	; --- Macros for inc/dec-rementing
                                 	; --- a byte in SRAM
                                 	.macro INCSRAM	; inc byte in SRAM
                                 		lds	r16,@0
                                 		inc	r16
                                 		sts	@0,r16
                                 	.endmacro
                                 
                                 	.macro DECSRAM	; dec byte in SRAM
                                 		lds	r16,@0
                                 		dec	r16
                                 		sts	@0,r16
                                 	.endmacro
                                 
                                 	; ---------------------------------------
                                 	; --- Code
                                 	.cseg
                                 	.org 	$0
000000 940c 0004                 	jmp	START
                                 	.org	INT0addr
000002 940c 002d                 	jmp	MUX
                                 
                                 
                                 START:
                                 	; stt stackpekaren
000004 e004                      	ldi r16, HIGH(RAMEND)
000005 bf0e                      	out SPH, r16
000006 e50f                      	ldi r16, LOW(RAMEND)
000007 bf0d                      	out SPL, r16
                                 
000008 940e 00b9                 	call	HW_INIT	
00000a 940e 00c5                 	call	WARM
                                 RUN:
                                 /*	call BEEP
                                 	rjmp RUN*/
00000c 940e 0051                 	call	JOYSTICK
00000e 940e 00ef                 	call	ERASE_VMEM
000010 940e 009f                 	call	UPDATE
                                 
                                 	; Vnta en stund s inte spelet gr fr fort 
000012 e406                      	ldi r16, GAME_SPEED
                                 RUN_DELAY:
000013 940e 0104                 	call DELAY
000015 950a                      	dec r16
000016 f7e1                      	brne RUN_DELAY
                                 	
                                 	
                                 	; Avgr om trff
                                 	; Kolla om X-led stmmer verens
000017 e0b0                      	ldi XH, HIGH(POSX)
000018 e6a0                      	ldi XL, LOW(POSX)
000019 910c                      	ld r16, X
00001a 9612                      	adiw X, 2
00001b 911c                      	ld r17, X
00001c 1701                      	cp r16, r17
00001d f469                      	brne NO_HIT
                                 
                                 	; Kolla om Y-led stmmer verens (POSX + 1 = Y-axis)
00001e e0b0                      	ldi XH, HIGH(POSX)
00001f e6a0                      	ldi XL, LOW(POSX)
000020 95a3                      	inc XL
                                 
000021 910c                      	ld r16, X
000022 9612                      	adiw X, 2
000023 911c                      	ld r17, X
000024 1701                      	cp r16, r17
                                 
000025 f429                      	brne	NO_HIT	
000026 e604                      	ldi	r16,BEEP_LENGTH
000027 940e 00f8                 	call	BEEP
000029 940e 00c5                 	call	WARM
                                 NO_HIT:
00002b 940c 000c                 	jmp	RUN
                                 

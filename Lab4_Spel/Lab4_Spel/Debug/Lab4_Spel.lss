                                 	; skriv rutin som handhar multiplexningen och ***
                                 	; utskriften till diodmatrisen. ka SEED.		***
00002d 930f                      	push r16
00002e b70f                      	in r16, SREG
00002f 930f                      	push r16
000030 93bf                      	push XH
000031 93af                      	push XL
                                 	
